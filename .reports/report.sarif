            {
              "id": "todo-comment",
              "name": "TODO Comment Detection",
              "shortDescription": {
                "text": "Detects TODO comments in code"
              },
              "fullDescription": {
                "text": "This rule identifies TODO comments that may indicate incomplete work"
              },
              "help": {
                "text": "Consider addressing or documenting TODO items before production deployment"
              }
            },
            {
              "id": "direct-db-access",
              "name": "Direct Database Access Detection",
              "shortDescription": {
                "text": "Detects direct database access outside repositories"
              },
              "fullDescription": {
                "text": "This rule identifies potential violations of repository pattern by detecting direct database calls"
              },
              "help": {
                "text": "Use repository interfaces instead of direct database access for better testability and separation of concerns"
              }
            },
            {
              "id": "missing-error-handling",
              "name": "Missing Error Handling Detection",
              "shortDescription": {
                "text": "Detects async operations without proper error handling"
              },
              "fullDescription": {
                "text": "This rule identifies async operations that may not have proper error handling"
              },
              "help": {
                "text": "Use try-catch blocks or proper error handling for async operations"
              }
            },
            {
              "id": "hardcoded-secrets",
              "name": "Hardcoded Secrets Detection",
              "shortDescription": {
                "text": "Detects potential hardcoded secrets"
              },
              "fullDescription": {
                "text": "This rule identifies potential hardcoded passwords, API keys, and other secrets"
              },
              "help": {
                "text": "Use environment variables or secure configuration management for secrets"
              }
            },
            {
              "id": "console-usage",
              "name": "Console Usage Detection",
              "shortDescription": {
                "text": "Detects console.log statements"
              },
              "fullDescription": {
                "text": "This rule identifies console.log statements that should be replaced with proper logging"
              },
              "help": {
                "text": "Use the logging framework instead of console.log for better observability"
              }
            }
          ]
        }
